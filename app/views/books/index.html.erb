<h1>Books</h1>
<table>
  <thead>
  <tr>
    <th>title</th>
    <span><%= @book.title %></span>
     <th>body</th>
     <th colspan="3"></th>
  </tr>
  </thead>

</table>
<!--投稿フォームはここから-->

<h1>New book</h1>
<%= form_with model: @book do |f| %>
<p>Title</p>
<%= f.text_field :title %>
<p>Body</p>
<%= f.text_area :body %><br>

<%= f.submit 'Create Book' %>
<%end%>
